{
  "name": "screencast-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "dev-lti": "NODE_ENV=development node lti.js",
    "prod-lti": "NODE_ENV=production node lti.js",
    "bull-monitor": "node bull-monitor.js",
    "migration:tenant_db:generate": "ENV=1 DB_GROUP=tenant_db npx sequelize-cli migration:generate",
    "migration:master_db:generate": "ENV=1 DB_GROUP=master_db npx sequelize-cli migration:generate",
    "migration:tenant_db:migrate": "ENV=1 DB_GROUP=tenant_db npx sequelize-cli db:migrate",
    "migration:master_db:migrate": "ENV=1 DB_GROUP=master_db npx sequelize-cli db:migrate",
    "migration:new_tenant_db:migrate": "ENV=1 DB_GROUP=new_tenant_db npx sequelize-cli db:migrate",
    "migration:tenant_db:seed:generate": "ENV=1 DB_GROUP=tenant_db npx sequelize-cli seed:generate",
    "migration:master_db:seed:generate": "ENV=1 DB_GROUP=master_db npx sequelize-cli seed:generate",
    "migration:tenant_db:seed:all": "ENV=1 DB_GROUP=tenant_db npx sequelize-cli db:seed:all",
    "migration:master_db:seed:all": "ENV=1 DB_GROUP=master_db npx sequelize-cli db:seed:all",
    "migration:tenant_db:model:generate": "ENV=1 DB_GROUP=tenant_db npx sequelize-auto --output './models/tenant_db' --dialect mysql --lang esm --useDefine esm",
    "migration:master_db:model:generate": "ENV=1 DB_GROUP=master_db npx sequelize-auto --output './models/master_db' --dialect mysql --lang esm --useDefine esm"
  },
  "config": {
    "migration_name": "xxxx"
  },
